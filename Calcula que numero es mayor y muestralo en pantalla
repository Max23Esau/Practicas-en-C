#include <stdio.h>

int main() {

 int num1, num2, mayor;
 
 printf("Ingrese un numero: ");
 scanf("%i", &num1);
 
 printf("Ingrese otro numero: ");
 scanf("%i", &num2);
 
 if(num1 > num2)
 {
   printf("%i es mayor que %i", num1, num2);
 }
 else if(num1 < num2)
 {
   printf("%i es mayor que %i", num2, num1);
 }
 else
 {
 printf("Los dos son iguales !");
 }
 
 return 0;
}
